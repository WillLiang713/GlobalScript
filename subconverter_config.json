{
  "options": {
    "skip_cert_verify": true,
    "dns": {
      "enable": true,
      "listen": "0.0.0.0:1053",
      "ipv6": true,
      "enhanced_mode": "fake-ip",
      "nameserver": ["tls://223.5.5.5"],
      "fallback": ["tls://4.2.2.1", "tls://8.8.8.8", "tls://9.9.9.9"],
      "fake_ip_range": "198.18.0.1/16",
      "fake_ip_filter": [
        "+.lan",
        "+.local",
        "+.msftconnecttest.com",
        "+.msftncsi.com",
        "localhost.ptlogin2.qq.com",
        "localhost.sec.qq.com",
        "localhost.work.weixin.qq.com",
        "*.localdomain",
        "*.example",
        "*.invalid",
        "*.localhost",
        "*.test",
        "*.local",
        "*.home.arpa"
      ]
    }
  },
  "proxy_filter": [
    "^(?=.*(美国|Japan|日本|Singapore|新加坡|Hong Kong|香港|Taiwan|台湾))(?!.*(流量|到期|剩余|套餐|expire|traffic|quota|有效期)).*$"
  ],
  "proxy-groups": [
    {
      "name": "节点选择",
      "type": "select",
      "proxies": [
        "延迟选优",
        "手动选择",
        "故障转移",
        "负载均衡(散列)",
        "负载均衡(轮询)"
      ]
    },
    {
      "name": "手动选择",
      "type": "select",
      "include-all": true
    },
    {
      "name": "漏网之鱼",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "Bing",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "Github",
      "type": "select",
      "proxies": ["节点选择", "手动选择", "全局直连", "延迟选优", "故障转移"]
    },
    {
      "name": "谷歌服务",
      "type": "select",
      "proxies": ["节点选择", "手动选择", "全局直连", "延迟选优", "故障转移"]
    },
    {
      "name": "苹果服务",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "微软服务",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "Onedrive",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "AI",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "Bilibili",
      "type": "select",
      "proxies": ["全局直连"]
    },
    {
      "name": "Zoom",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "YouTube",
      "type": "select",
      "proxies": ["节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "Netflix",
      "type": "select",
      "proxies": ["节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "TikTok",
      "type": "select",
      "proxies": ["节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "Spotify",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "游戏服务",
      "type": "select",
      "proxies": ["全局直连", "节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "电报消息",
      "type": "select",
      "proxies": ["节点选择", "手动选择", "延迟选优", "故障转移"]
    },
    {
      "name": "负载均衡(轮询)",
      "type": "load-balance",
      "strategy": "round-robin",
      "include-all": true
    },
    {
      "name": "负载均衡(散列)",
      "type": "load-balance",
      "strategy": "consistent-hashing",
      "include-all": true
    },
    {
      "name": "故障转移",
      "type": "fallback",
      "include-all": true
    },
    {
      "name": "全局直连",
      "type": "select",
      "proxies": ["DIRECT", "REJECT", "节点选择", "手动选择"]
    },
    {
      "name": "延迟选优",
      "type": "url-test",
      "interval": 240,
      "tolerance": 80,
      "include-all": true
    }
  ],
  "rules": [
    "RULE-SET,ipdirect,全局直连,no-resolve",
    "RULE-SET,ipprivate,全局直连,no-resolve",
    "RULE-SET,telegramcidr,电报消息,no-resolve",
    "RULE-SET,direct,全局直连",
    "RULE-SET,private,全局直连",
    "RULE-SET,google,谷歌服务",
    "RULE-SET,apple,苹果服务",
    "RULE-SET,bing,Bing",
    "RULE-SET,github,Github",
    "RULE-SET,onedrive,Onedrive",
    "RULE-SET,microsoft,微软服务",
    "RULE-SET,ai,AI",
    "RULE-SET,youtube,YouTube",
    "RULE-SET,netflix_ip,Netflix",
    "RULE-SET,netflix_site,Netflix",
    "RULE-SET,tiktok,TikTok",
    "RULE-SET,spotify,Spotify",
    "RULE-SET,games,游戏服务",
    "RULE-SET,bilibili,Bilibili",
    "RULE-SET,zoom,Zoom",
    "RULE-SET,proxy,节点选择",
    "RULE-SET,gfw,节点选择",
    "RULE-SET,tld-not-cn,节点选择",
    "MATCH,漏网之鱼"
  ]
}
